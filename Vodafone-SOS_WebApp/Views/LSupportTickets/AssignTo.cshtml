@{
    ViewBag.Title = "AssignTo";
}

@using (Html.BeginForm())
{
    @Html.Hidden("TransactionId")
    <div class="form-horizontal">
        <div class="form-group">
            @Html.Label("Assignee List :", new { @Class = "col-md-2" })
            <div class="col-md-10">
                @Html.DropDownList("Assignees", null, "-- Select Here --", new { @class = "form-control " })
            </div>
        </div>
        <div class="form-group">
            <br><button type="button" class="btn btn-borderRed btn-cons" value="Back" id="bck" onclick="window.location.href = '@HttpContext.Current.Session["from"]'">Back &nbsp;</button>
            <button type="submit" class="btn btn-red btn-cons">Submit &nbsp;</button>
        </div>

    </div>
}



