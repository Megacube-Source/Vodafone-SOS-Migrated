@model Vodafone_SOS_WebApp.ViewModels.RCommissionTypeViewModel

@{
    ViewBag.Title = "Delete";
}


<h3>Are you sure you want to delete this Commission Type ?</h3>
<div>
   @using (Html.BeginForm())
   {
    @Html.AntiForgeryToken()
    @Html.HiddenFor(m=>m.Id)
      @Html.HiddenFor(m=>m.RctDescription) 
      @Html.HiddenFor(m=>m.RctName)
      @Html.HiddenFor(m=>m.GcCompanyName)
    <h4 class="text-danger">@ViewData["ErrorMessage"]</h4>
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.RctName)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.RctName)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.RctDescription)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.RctDescription)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.GcCompanyName)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.GcCompanyName)
        </dd>

    </dl>


    <div class="col-md-4">
        <button type="button" class="btn btn-borderRed btn-cons" onclick="window.location.href = '@HttpContext.Current.Session["from"]'">Back&nbsp;</button>
        <button type="submit" class="btn btn-red btn-cons">Delete&nbsp;</button>

    </div>
   }
</div>
