@model Vodafone_SOS_WebApp.ViewModels.RStatusOwnerViewModel

@{
    ViewBag.Title = "Create";
}




@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        <h4 class="text-danger">@ViewData["ErrorMessage"]</h4>
        <div class="form-group">
            @Html.LabelFor(model => model.RsoStatusOwner, htmlAttributes: new { @class = " col-md-2 required" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.RsoStatusOwner, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.RsoStatusOwner, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="col-md-4">
            <button type="button" class="btn btn-borderRed btn-cons" onclick="window.location.href = '@HttpContext.Current.Session["from"]'">Back&nbsp;</button>
            <button type="submit" class="btn btn-red btn-cons">Submit&nbsp;</button>
        </div>
    </div>
}


@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
