@{
    ViewBag.Title = "Delete Stage";

}
<h3>Are you sure you want to delete this Ticket Stage ?</h3>

<div>
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        @Html.HiddenFor(m => m.Id)
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(model => model.RtsName)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.RtsName)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.RtsDescription)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.RtsDescription)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.RtsIsActive)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.RtsIsActive)
            </dd>

        </dl>


        <div class="col-md-4">
            <button type="button" class="btn btn-borderRed btn-cons" onclick="window.location.href = '@HttpContext.Current.Session["from"]'">Back&nbsp;</button>
